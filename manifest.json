{
    "manifest_version": 3,
    "name": "Site Ziyaret Sayacı",
    "version": "1.0",
    "description": "Hangi siteye kaç kez girdiğini takip eder.",
    "permissions": ["tabs", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon.png"
    }
  }
  